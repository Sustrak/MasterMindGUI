<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MainMenuViewController">
   <children>
      <VBox alignment="CENTER" layoutX="350.0" layoutY="96.0" spacing="20.0">
         <children>
            <Button fx:id="codeBreakerButton" mnemonicParsing="false" onAction="#codeBreakerButtonAction" prefWidth="200.0" text="CodeBreaker" />
            <Button fx:id="codeMakerButton" mnemonicParsing="false" onAction="#codeMakerButtonAction" prefWidth="200.0" text="CodeMaker" />
            <Button fx:id="loadGameButton" mnemonicParsing="false" onAction="#loadGameButtonAction" prefWidth="200.0" text="Cargar partida" />
            <Button fx:id="rankButton" mnemonicParsing="false" onAction="#rankButtonAction" prefWidth="200.0" text="Consultar rankings" />
            <Button fx:id="recordsButton" mnemonicParsing="false" onAction="#recordsButtonAction" prefWidth="200.0" text="Consultar Records" />
            <Button fx:id="playerButton" mnemonicParsing="false" onAction="#playerButtonAction" prefWidth="200.0" text="Consultar jugador" />
            <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#settingsButtonAction" prefWidth="200.0" text="Ajustes" />
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#logoutButtonAction" prefWidth="200.0" text="Logout" />
         </children>
      </VBox>
   </children>
</AnchorPane>
